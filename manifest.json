{
  "name": "ikonwu",
  "description": "BY mixlab",
  "version": "0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icons/logo.png",
    "24": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "tabs",
    "webRequestBlocking",
    "webRequest",
    "contextMenus",
    "activeTab",
    "desktopCapture",
    "tabCapture",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "css": [
        "css/content_css.css"
      ],
      "js": [
       "js/baseImg.js","js/imageAllExchange.js","js/content_script.js"
      ],
      "matches" : ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "persistent": true,
    "scripts": ["lib/face-api.min.js","js/imageAllExchange.js","js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "ikonwu"
  }
}